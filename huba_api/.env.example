# Huba API Configuration
PORT=3001
NODE_ENV=development

# Supabase Configuration (Huba's own database)
SUPABASE_URL=your_huba_supabase_url
SUPABASE_PUBLISHABLE_KEY=your_huba_publishable_key
SUPABASE_SECRET_KEY=your_huba_secret_key

# KGiTON Core API (for token validation)
KGITON_API_URL=http://localhost:3000

# Webhook Configuration
# Shared secret for verifying HMAC signatures from Core API
# Must match Core API's HUBA_WEBHOOK_SECRET
KGITON_WEBHOOK_SECRET=your_webhook_secret_here
